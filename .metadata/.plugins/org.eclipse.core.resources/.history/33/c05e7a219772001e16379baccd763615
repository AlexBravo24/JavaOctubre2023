package com;

public class VideoJuego {
	
	private String juego;
	private String personaje;
	private String nombrepersonaje;
	private String clasepersonaje;
	private String atributospersonaje;
	
	public VideoJuego() {}

	public VideoJuego(String juego, String personaje, String nombrepersonaje, String clasepersonaje,
			String atributospersonaje) {
		super();
		this.juego = juego;
		this.personaje = personaje;
		this.nombrepersonaje = nombrepersonaje;
		this.clasepersonaje = clasepersonaje;
		this.atributospersonaje = atributospersonaje;
	}

	public String getJuego() {
		return juego;
	}

	public void setJuego(String juego) {
		this.juego = juego;
	}

	public String getPersonaje() {
		return personaje;
	}

	public void setPersonaje(String personaje) {
		this.personaje = personaje;
	}

	public String getNombrepersonaje() {
		return nombrepersonaje;
	}

	public void setNombrepersonaje(String nombrepersonaje) {
		this.nombrepersonaje = nombrepersonaje;
	}

	public String getClasepersonaje() {
		return clasepersonaje;
	}

	public void setClasepersonaje(String clasepersonaje) {
		this.clasepersonaje = clasepersonaje;
	}

	public String getAtributospersonaje() {
		return atributospersonaje;
	}

	public void setAtributospersonaje(String atributospersonaje) {
		this.atributospersonaje = atributospersonaje;
	}

	@Override
	public String toString() {
		return "VideoJuego [juego=" + juego + ", personaje=" + personaje + ", nombrepersonaje=" + nombrepersonaje
				+ ", clasepersonaje=" + clasepersonaje + ", atributospersonaje=" + atributospersonaje + "]";
	}
	
	
	

}
